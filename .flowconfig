[include]
./source/
./tests/

[ignore]
./public/
./build/
./node_modules/

[lints]
sketchy-null=warn
sketchy-null-bool=off
untyped-type-import=warn
unclear-type=warn
nonstrict-import=warn
unnecessary-optional-chain=warn
non-array-spread=warn

[options]
emoji=true
exact_by_default=true
include_warnings=true
